// Physical propertiescreate a,bforall {nseg=1 L=20 diam=20 insert hh}objref g[2]for i=0,1 g[i] = new gap()a g[0].loc(.999)	/*distal end of a*/b g[1].loc(.001)  /*proximal end of b*/setpointer g[0].vgap, b.v(.001)setpointer g[1].vgap, a.v(.999)access a// run parameterststop=50// simulation controlobjref stima stim = new IClamp(.05){stim.del=10 stim.dur=2 stim.amp=.1}xopen("06_gap.ses")	/* Open the session file  */